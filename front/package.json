{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "concurrently": "^4.1.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-css": "node-sass --include-path scss src/scss/index.scss src/App.css",
    "build-watch": "nodemon -e scss -x \"npm run build-css\"",
    "serv": "cd backend && nodemon server.js",
    "dev": "concurrently \" npm run start\" \" npm run build-watch\" \"npm run serv\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
